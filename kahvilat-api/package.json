{
  "name": "kahvilat-api",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "format": "prettier \"{src,tests}/**/*.ts\" --check",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "start": "serverless offline --httpPort 3010 --lambdaPort 3011 start",
    "test:unit": "jest \"/tests/unit\"",
    "test:integration": "jest \"/tests/integration\"",
    "typecheck": "tsc --noEmit",
    "typegen": "npx openapi-client-axios-typegen src/specification/openapi.json > src/types/openapi.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.345.0",
    "ajv": "^8.12.0",
    "aws-cdk": "^2.82.0",
    "aws-cdk-lib": "^2.82.0",
    "constructs": "^10.0.0",
    "handlebars": "^4.7.7",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.11",
    "openapi-backend": "^5.9.2",
    "qs": "^6.11.2",
    "redis": "^4.6.7",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.4",
    "@types/qs": "^6.9.7",
    "@types/redis-mock": "^0.17.1",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "openapi-client-axios": "^7.3.2",
    "openapi-client-axios-typegen": "^7.3.2",
    "prettier": "^2.8.8",
    "redis-mock": "^0.56.3",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.0"
  }
}
